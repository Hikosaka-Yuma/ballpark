<%= render "leftcontents" %>

<div class = "community-container">
  <div class = "right-contents">
    <div class = "post-all">
      <% @team.posts. each do |post| %>
        <div class = "post-index">
          <p class = "post-date">
            <%= post.created_at.strftime('%Y年 %m月 %d日 %H時 %M分') %>
          <p>
          <div class = "post-contents">
            <p class = "post-titile">
              <%= post.title %>
            </p>
            <p class = "post-description">
              <%= post.description %>
            </p>
          </div>
        </div>
      <% end %>
    </div>
  </div>
</div>